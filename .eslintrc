{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "blockBindings": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}